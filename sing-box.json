{
  "log": {
    "level": "info"
  },
  "dns": {
    "strategy": "prefer_ipv4",
    "servers": [
      {
        "type": "udp",
        "tag": "dns-direct",
        "server": "223.5.5.5"
      },
      {
        "type": "tls",
        "tag": "dns-remote",
        "server": "8.8.8.8",
        "detour": "美国 12 家宽"
      },
      {
        "type": "fakeip",
        "tag": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip"
      }
    ],
    "final": "dns-direct"
  },
  "inbounds": [
    {
      "type": "tproxy",
      "tag": "tproxy-in4",
      "listen": "0.0.0.0",
      "listen_port": 19000,
      "sniff": true,
      "sniff_override_destination": true
    },
    {
      "type": "tproxy",
      "tag": "tproxy-in6",
      "listen": "::",
      "listen_port": 19001,
      "sniff": true,
      "sniff_override_destination": true
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "default_domain_resolver": "dns-direct",
    "rules": [
      {
        "inbound": "tproxy-in4",
        "outbound": "美国 12 家宽"
      },
      {
        "inbound": "tproxy-in6",
        "outbound": "美国 12 家宽"
      }
    ],
    "final": "direct"
  }
}
